{
    "name": "jascha030/iterable",
    "description": "Utilities for working with iterators and generators.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jascha030\\Iterable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jascha030\\Iterable\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jascha030",
            "email": "contact@jaschavanaalst.nl"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "platform-check": false,
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false,
            "forward-command": true
        }
    },
    "require": {
        "php": "~8.1||~8.2",
        "phpoption/phpoption": "^1.9"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^6.0"
    },
    "scripts": {
        "php-cs-fixer": "tools/php-cs-fixer",
        "phpunit": "tools/phpunit.phar",
        "rector": "vendor-bin/rector/vendor/bin/rector",
        "phpstan": "vendor-bin/phpstan/vendor/bin/phpstan",
        "format": "@php-cs-fixer fix --config=.php-cs-fixer.php",
        "test": "@phpunit -c phpunit.xml",
        "analyze": "@phpstan analyze -c phpstan.neon",
        "fix:dry": "@rector process src --dry-run",
        "fix": "@rector process src"
    }
}
